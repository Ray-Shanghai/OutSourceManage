	$("input[name='xqbkxblkuan']").val(gbcxblkuan);
	$("input[name='xqbkxblhou']").val(gbcxblhou);
	
	/*箱子外径长=箱子内径长+小墙板厚*2+小墙板长项边料厚*2，箱子外径宽=箱子内径宽+大墙板长项边料厚*2+大墙板厚*2，
		箱子外径厚=箱子内径厚+盖板厚+底板厚+长方厚+短方厚+盖板长项边料厚（如果有盖板）。 */
