       <script>
       layui.config({
			base : 'js/'
		});    
		layui.use([ 'jquery','paging', 'form', 'element'],
				function() {
					var $ = layui.jquery, element = layui.element(); //Tab的切换功能，切换事件监听等，需要依赖element模块
							paging = layui.paging(),
							layerTips = parent.layer === undefined ? layui.layer
									: parent.layer, //获取父窗口的layer对象
							layer = layui.layer, //获取当前窗口的layer对象
							form = layui.form();
							getData({});
							
					function getData(data){
						paging.init({
							url : base.serverContet
									+ '/manager/loginIn/SdOrderInfo/queryListByTrack.do', //地址
							elem : '#content', //内容容器
							params :data,
							type : 'POST',
							tempElem : '#tpl', //模块容器
							pageConfig : { //分页参数配置
								elem : '#paged', //分页容器
								pageSize : base.pageSize,
								pageIndex:1
							//分页大小
							}, 
							success : function(data) { //渲染成功的回调
								//alert("回调成功")
								
							},
							fail : function(msg) { //获取数据失败的回调
								layer.alert('登陆超时，请重新登陆', {icon: 2});
							},
							complate : function() { //完成的回调
								
							},
							serverError: function(xhr, status, error) { //ajax的服务错误
								layer.alert('服务错误', {icon: 2});
							}
						});
				
					}
					$("#search").click(function(){
						  getData($("#form1").serializeObject());
					});
				
		});
			
			 function seeInfo(orderId) {
				layer.open({
					type: 2,
					title: '跟踪详情',
					shadeClose: true,
					shade: [0.6, '#393D49'],
					maxmin: true, //开启最大化最小化按钮
					area: ['1200px', '80%'],
					content: 'afterOrderSee.html?'+orderId,
					btn: ['确定','取消'],
					btnAlign: 'c',
					skin: 'layui-layer-red'
				});
			}
			 /*去往修改订单页*/
			function editOrderInfo(orderId){
				layer.open({
					type: 2,
					title: '跟踪详情',
					shadeClose: true,
					shade: [0.6, '#393D49'],
					maxmin: true, //开启最大化最小化按钮
					area: ['1200px', '80%'],
					content: 'afterOrderEdit.html?'+orderId,
					/* btn: ['确定','取消'],
					btnAlign: 'c', */
					skin: 'layui-layer-red'
				});
			}
			/*删除订单*/
			function deleteOrderInfo(orderId){
				$.ajax({type : 'POST',
					url : base.serverContent+ '/manager/loginIn/SdOrderInfo/deleteOrderInfo.do',
					data :{orderId:orderId},
					async : false,
					success : function(data) {
							if (data.code == "000") {
								
								layer.alert('订单废除成功',{icon : 6},
										function(index){
										layer.close(index);
										window.location.reload();}
												);
								 
							} else {
								layer.alert('订单废除失败',{icon : 2});
								 
							 }
						},
						error : function() {
						}
					});
			}
       </script>
	</body>
</html>
