    <script>
		
		$('.icon-icon-test').click(function(){
			$(this).parent().parent().children('.list-content ul').hide();
		})
		
		
	$(".icon-icon-test").click(function(){
		if($(this).hasClass('rotate')){
			$(this).parent().parent().children('.list-content ul').hide();
			$(this).removeClass('rotate').addClass('rotate1');
		}else{
			$(this).parent().parent().children('.list-content ul').show();
			$(this).removeClass('rotate1').addClass('rotate');
		}
		
	})
	
	$('.check-material li').click(function(){
    	$('.bg-tc').show();
	   
	   var swiper = new Swiper('.swiper-container', {
	        pagination: '.swiper-pagination',
	        paginationClickable: true,
	        observer: true,//修改swiper自己或子元素时，自动初始化swiper
            observeParents: true//修改swiper的父元素时，自动初始化swiper
	   });
	})
	
	$('.close-btn .icon-chahao').click(function(){
		$('.bg-tc').hide();
	})
	
	$('.select-quotation li').click(function(){
		if($(this).children('.table-top').children('.icon-duihao').length==0){
		 $(this).children('.table-top').prepend('<i class="iconfont icon-duihao"></i>')
		 $(this).siblings().children('.table-top').children('.icon-duihao').remove()
		}
	})
    </script>
