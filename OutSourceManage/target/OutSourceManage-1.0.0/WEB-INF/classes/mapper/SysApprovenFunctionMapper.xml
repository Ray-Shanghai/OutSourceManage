<?xml version="1.0" encoding="UTF-8" ?> 

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="com.sd.farmework.mapper.SysApprovenFunctionMapper"> 

		<resultMap type="com.sd.farmework.pojo.SysApprovenFunction" id="SysApprovenFunctionMap"> 

		<result column="APPROVEN_FUNCTION_ID" property="approvenFunctionId"/>
		<result column="APPROVEN_FUNCTION_NAME" property="approvenFunctionName"/>
		
		<result column="APPROVEN_OPERATE_FUNCTION_ID" property="approvenOperateFunctionId"/>
		<result column="APPROVEN_LIST_FUNCTION_ID" property="approvenListFunctionId"/>
		<result column="APPROVEN_LISTED_FUNCTION_ID" property="approvenListedFunctionId"/>
		<!-- 系统公共属性 必须按照规定存取数据start -->
		<result column="REMARK" property="remark"/>
		<result column="STATUS" property="status"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="CREATE_USER_ID" property="createUserId"/>
		<result column="CREATE_USER_NAME" property="createUserName"/>
		<result column="CREATE_TEAM_ID" property="createTeamId"/>
		<result column="CREATE_TEAM_NAME" property="createTeamName"/>
		<result column="CREATE_DEPART_ID" property="createDepartId"/>
		<result column="CREATE_DEPART_NAME" property="createDepartName"/>
		<result column="CREATE_UNIT_ID" property="createUnitId"/>
		<result column="CREATE_UNIT_Name" property="createUnitName"/>
		<result column="LAST_UPDATE_TIME" property="lastUpdateTime"/>
		<result column="LAST_UPDATE_USER_ID" property="lastUpdateUserId"/>
		<result column="LAST_UPDATE_TEAM_ID" property="lastUpdateTeamId"/>
		<result column="LAST_UPDATE_TEAM_NAME" property="lastUpdateTeamName"/>
		<result column="LAST_UPDATE_DEPART_ID" property="lastUpdateDepartId"/>
		<result column="LAST_UPDATE_DEPART_NAME" property="lastUpdateDepartName"/>
		<result column="LAST_UPDATE_UNIT_ID" property="lastUpdateUnitId"/>
		<result column="LAST_UPDATE_UNIT_Name" property="lastUpdateUnitName"/>
		<!-- 系统公共属性 必须按照规定存取数据end -->

	</resultMap>
	
	<select id="queryList" parameterType="com.sd.farmework.pojo.SysApprovenFunction"  resultMap="SysApprovenFunctionMap">
		SELECT 
		t.APPROVEN_FUNCTION_ID,
		t.APPROVEN_FUNCTION_NAME,
		t.REMARK,
		t.CREATE_TIME,
		t.CREATE_USER_ID,
		t.CREATE_USER_NAME,
		t.LAST_UPDATE_TIME,
		t.LAST_UPDATE_USER_ID,
		t.LAST_UPDATE_USER_NAME,
		APPROVEN_OPERATE_FUNCTION_ID,
		APPROVEN_LIST_FUNCTION_ID,
		APPROVEN_LISTED_FUNCTION_ID
		FROM  SYS_APPROVEN_FUNCTION t where 1=1 
		<if test="approvenFunctionId!=null and approvenFunctionId!=''">
			and APPROVEN_FUNCTION_ID = #{approvenFunctionId}
		</if>
		<if test="approvenOperateFunctionId!=null and approvenOperateFunctionId!=''">
			and APPROVEN_OPERATE_FUNCTION_ID = #{approvenOperateFunctionId}
		</if>
			<if test="approvenListFunctionId!=null and approvenListFunctionId!=''">
			and APPROVEN_LIST_FUNCTION_ID = #{approvenListFunctionId}
		</if>
			<if test="approvenListedFunctionId!=null and approvenListedFunctionId!=''">
			and APPROVEN_LISTED_FUNCTION_ID = #{approvenListedFunctionId}
		</if>
		
		 order by t.APPROVEN_FUNCTION_ID 
	</select>
</mapper> 

